function bubble(arr){
    for (let i=0; i<arr.length; ++i){
        for(let j =0; j<arr.length -1 -i; j++){
              if (arr[j] > arr[j+1]){
                  const tmp=arr[j];
                  arr[j] =arr[j+1];
                  arr[j+1] = tmp
              }
          }
    }
    return arr
}
console.log(bubble([5,2,1,3,5]))

